{
  "name": "dansk-metal",
  "version": "1.11.0",
  "private": true,
  "scripts": {
    "clear:install": "npx clear && npm install",
    "clear": "npx --yes rimraf ./**/node_modules",
    "commit": "git-cz",
    "dev": "npx nx run-many --parallel --target=serve --projects=web",
    "gen": "graphql-codegen",
    "generate": "npx nx run-many --target=generate --all --parallel=true",
    "vercel:env_link": "npx --yes cross-env ./scripts/vercel/env_link.sh",
    "vercel:env_pull": "npx --yes cross-env ./scripts/vercel/env_pull.sh",
    "web:build": "npx nx run web:build:production",
    "web:serve": "npx nx run web:serve"
  },
  "workspaces": {
    "packages": [
      "apps/web",
      "libs/**/*"
    ]
  },
  "dependencies": {
